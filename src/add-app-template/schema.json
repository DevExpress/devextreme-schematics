{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsDevextremeAddAppTemplate",
  "title": "Include a DevExtreme Layout Template in the Anular application.",
  "type": "object",
  "properties": {
    "layout": {
      "type": "string",
      "description": "The type of DevExtreme layout to include",
      "default": "side-nav-outer-toolbar"
    },
    "dxversion": {
      "type": "string",
      "description": "The version of DevExtreme",
      "default": "18.2-unstable"
    },
    "project": {
      "type": "string",
      "description": "The name of project to modify"
    },
    "empty": {
      "type": "boolean",
      "description": "Skip sample views generation",
      "default": false
    }
  }
}
